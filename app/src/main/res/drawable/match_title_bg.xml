<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#f5f5f5"/>
            <stroke
                android:width="1dp"
                android:color="#dddddd"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:right="1dp"
        android:bottom="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#f5f5f5"/>
            <stroke
                android:width="1dp"
                android:color="#f5f5f4"/>
        </shape>
    </item>
</layer-list>